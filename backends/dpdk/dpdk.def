class DpdkHeaderType : Type_Header {
    std::ostream& toSexp(std::ostream& out) const;
#nodbprint
}

class DpdkStructType : Type_Struct {
    std::ostream& toSexp(std::ostream& out) const;
#nodbprint
}

class DpdkAsmStatement {
    virtual std::ostream& toSexp(std::ostream& out) const;
#nodbprint
}

class DpdkAction {
    inline IndexedVector<DpdkAsmStatement> statements;
    inline cstring name;
    inline ParameterList para;
    std::ostream& toSexp(std::ostream& out) const;
#nodbprint
}

class DpdkTable{
    inline cstring name;
    Key match_keys;
    ActionList actions;

    std::ostream& toSexp(std::ostream& out) const;
#nodbprint
#novalidate
}

// Toplevel Assembly representation
class DpdkAsmProgram : P4Program {
    inline IndexedVector<DpdkHeaderType> headerType;
    inline IndexedVector<DpdkStructType> structType;
    inline IndexedVector<DpdkAction> actions;
    inline IndexedVector<DpdkTable> tables;
    inline IndexedVector<DpdkAsmStatement> statements;
// Globals
    std::ostream& toSexp(std::ostream& out) const;
#nodbprint
}

class DpdkListStatement : DpdkAsmStatement {
    optional cstring name;
    optional inline IndexedVector<DpdkAsmStatement> statements;
    std::ostream& toSexp(std::ostream& out) const override;
#nodbprint
}

class DpdkMovStatement : DpdkAsmStatement {
    Expression left;
    Expression right;
    std::ostream& toSexp(std::ostream& out) const override;
#nodbprint
}

